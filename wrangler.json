{
  "name": "shopify-app-cloudflare",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat", "nodejs_compat_populate_process_env"],
  "main": "./server.ts",
  "assets": {
    "directory": "./build/client"
  },
	"kv_namespaces": [
		{
			"binding": "SESSIONS",
			"id": "<UUID>"
		}
	],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}
